{
    "manifest_version": 2,
  
    "name": "Quora Downloader",
    "description": "This extension tracks your overall spendings.",
    "version": "1.0",
    "icons": {
    //   "128": "icon128.png",
      "48": "icon48.png"
    //   "16": "icon16.png"
    },
  
    "browser_action": {
      "default_icon": "icon48.png",
      "default_popup": "popup.html"
    },
  
    "background": {
      "scripts": ["eventPage.js", "content.js"],
      "persistent": false
    },

    "content_scripts":[
      {
        "matches": ["https://www.quora.com/*"],
        "css": ["quora.css"]
      }
    ],
  
    "permissions": [
            "storage",
            "notifications",
            "contextMenus",
            "tabs",
            "https://www.quora.com/*"
          ]
  
    // "options_page": "options.html"
  }